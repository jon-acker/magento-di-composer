<config>
    <modules>
        <Inviqa_Zygourator>
            <version>1.6.0.0</version>
        </Inviqa_Zygourator>
    </modules>
    <global>
        <models>
            <zygourator>
                <class>Inviqa_Zygourator_Model</class>
            </zygourator>
        </models>
        <helpers>
            <zygourator>
                <class>Inviqa_Zygourator_Helper</class>
            </zygourator>
        </helpers>

        <blocks>
            <moneybookers><class>Inviqa_Zygourator_Block</class></moneybookers>
        </blocks>
        <events>
            <magento_bootup>
                <observers>
                    <inviqa_zygourator_observer>
                        <type>singleton</type>
                        <class>Inviqa_Zygourator_Model_Observer</class>
                        <method>handleStartup</method>
                    </inviqa_zygourator_observer>
                </observers>
            </magento_bootup>
        </events>
    </global>
    <frontend>

        <routers>
            <zygourator>
                <use>standard</use>
                <args>
                    <module>Inviqa_Zygourator</module>
                    <frontName>zygourator</frontName>
                </args>
            </zygourator>
        </routers>

    </frontend>

</config>
